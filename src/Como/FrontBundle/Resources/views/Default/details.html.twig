{% extends 'ComoFrontBundle::layout.html.twig' %}

{% block body %}
<div class="tabbable"> <!-- Only required for left/right tabs -->
  <ul class="nav nav-tabs">
    <li class="active"><a href="#tab1" data-toggle="tab">Information</a></li>
    {% if txaData[1] is defined %}
        <li><a href="#tab2" data-toggle="tab">Availability</a></li>
    {% endif%}
    <li><a href="#tab3" data-toggle="tab">Map</a></li>
    {% if providerDump[0] is defined %}
        <li><a href="#tab4" data-toggle="tab">Provider Dump</a></li>
    {% endif%}
  </ul>
  <div class="tab-content">
    <div class="tab-pane active" id="tab1">
      
      {% include 'ComoFrontBundle:Default:object_details.html.twig' with {'obj':obj} %}
    </div>
    {% if txaData[1] is defined %}
        <div class="tab-pane" id="tab2">
              {% include 'ComoFrontBundle:Default:object_availability.html.twig' with {'txaData':txaData[0], 'warnings':txaData[1], 'avail_dates':avail_dates} %}
        </div>
    {% endif%}
    <div class="tab-pane" id="tab3">
      {% include 'ComoFrontBundle:Default:object_map.html.twig' with {'obj':obj} %}
    </div>
    {% if providerDump[0] is defined %}
        <div class="tab-pane" id="tab4"> 
            <ul>
                {% include 'ComoFrontBundle:Default:provider_dump.html.twig' with {'providerDump':providerDump} only %}
            </ul>
        </div>
    {% endif%}
  </div>
</div>



  <hr>                       
{% endblock body%}            