<h1>ProductImage list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Market_variant_name</th>
            <th>Multimedia_id</th>
            <th>Server_path</th>
            <th>Attribute_id_multimedia_file</th>
            <th>Attribute_id_multimedia_content</th>
            <th>Attribute_id_size_orientation</th>
            <th>Width</th>
            <th>Height</th>
            <th>Alt_text</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('productimage_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.marketvariantname }}</td>
            <td>{{ entity.multimediaid }}</td>
            <td>{{ entity.serverpath }}</td>
            <td>{{ entity.attributeidmultimediafile }}</td>
            <td>{{ entity.attributeidmultimediacontent }}</td>
            <td>{{ entity.attributeidsizeorientation }}</td>
            <td>{{ entity.width }}</td>
            <td>{{ entity.height }}</td>
            <td>{{ entity.alttext }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('productimage_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('productimage_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('productimage_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
