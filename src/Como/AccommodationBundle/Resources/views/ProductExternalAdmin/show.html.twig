<h1>ProductExternal</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Txa_attribute</th>
            <td>{{ entity.txaattribute }}</td>
        </tr>
        <tr>
            <th>Provider_short_name</th>
            <td>{{ entity.providershortname }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('productexternal') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('productexternal_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('productexternal_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
